{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "initdb:dev": "rm ./db/dev.better-sqlite3; knex migrate:latest --env development; knex seed:run --specific=create_admin.js --env development",
    "migrate:test": "knex migrate:latest --env test",
    "browse": "sqlitebrowser ./db/dev.better-sqlite3 -t users",
    "seed": "knex seed:run"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^8.0.1",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "express": "~4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "~2.0.0",
    "jade": "~1.11.0",
    "knex": "^2.3.0",
    "morgan": "~1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "passport-stub": "^1.1.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20"
  }
}
